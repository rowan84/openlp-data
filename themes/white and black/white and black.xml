<?xml version="1.0" encoding="utf-8"?>
<theme version="2.0">
    <name>white and black</name>
    <background type="solid">
        <color>#ffffff</color>
    </background>
    <font type="main">
        <name>Arial</name>
        <color>#000000</color>
        <size>52</size>
        <bold>True</bold>
        <italics>False</italics>
